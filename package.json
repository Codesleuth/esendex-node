{
  "name": "esendex",
  "version": "0.1.0",
  "description": "Esendex Node.js client library.",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "rm -rf dist && cd src && tsc",
    "test": "cd dist && mocha --require source-map-support/register",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --require source-map-support/register -R spec dist/test/*.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codesleuth/esendex-node"
  },
  "bugs": {
    "url": "https://github.com/codesleuth/esendex-node/issues"
  },
  "keywords": [
    "sms",
    "business",
    "node",
    "js",
    "sdk",
    "rest",
    "api"
  ],
  "engines": {
    "node": ">=0.10 <=5"
  },
  "author": "David Wood <david.p.wood@gmail.com> (http://www.codesleuth.co.uk/)",
  "license": "MIT",
  "dependencies": {
    "xml2js": "^0.4.15"
  },
  "devDependencies": {
    "body-parser": "^1.14.1",
    "coveralls": "^2.11.4",
    "express": "^4.13.3",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2",
    "source-map-support": "^0.3.3",
    "tsd": "^0.6.5",
    "typescript": "^1.6.2"
  }
}
